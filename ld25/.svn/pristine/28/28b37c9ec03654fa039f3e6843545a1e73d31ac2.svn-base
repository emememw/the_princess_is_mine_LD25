package com.emveyh.ld25.entity;

public enum EnemyType {

	SILVER_KNIGHT(1,0,2,2,1),
	BLUE_KNIGHT(5,1,2,1,2),
	RED_MAGE(6,1,2,2,3),
	YELLOW_MAGE(7,1,3,3,4),
	PRINCE(1,1,3,3,5),
	ROGUE(3,1,4,4,6),
	DWARF(2,1,4,4,7),
	BARBARIAN(8,1,5,4,8),
	GOLDEN_KNIGHT(10,1,5,4,9),
	BLOOD_KNIGHT(11,1,6,4,10);
	
	private int textureX;
	private int textureY;
	private int hp;
	private int dmg;
	private int level;
	
	private EnemyType(int textureX, int textureY, int hp, int dmg, int level) {
		this.textureX = textureX;
		this.textureY = textureY;
		this.hp = hp;
		this.dmg = dmg;
		this.level = level;
	}

	public int getTextureX() {
		return textureX;
	}

	public int getTextureY() {
		return textureY;
	}

	public int getHp() {
		return hp;
	}

	public int getDmg() {
		return dmg;
	}

	public int getLevel() {
		return level;
	}
	
	
	
}
