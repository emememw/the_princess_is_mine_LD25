package com.emveyh.ld25.map;

public enum Wave {

	Wave1(1, 10),
	Wave2(2, 15),
	Wave3(3, 20),
	Wave4(4, 25),
	Wave5(5, 30),
	Wave6(6, 35),
	Wave7(7, 40),
	Wave8(8, 45),
	Wave9(9, 50),
	Wave10(10, 55);
	
	
	private int spawnAmount;
	private int index;
	
	private Wave(int index, int spawnAmount) {
		this.index = index;
		this.spawnAmount = spawnAmount;
	}

	public int getSpawnAmount() {
		return spawnAmount;
	}

	public int getIndex() {
		return index;
	}
	
	
	
}
